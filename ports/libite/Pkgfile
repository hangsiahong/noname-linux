name=libite
version=1.9.2
deps=musl
desc="init's dependence"
source="https://github.com/troglobit/libite/releases/download/v$version/libite-$version.tar.xz"

build() {
	cd $name-$version
	./configure --prefix=/ --sbindir=/bin --disable-static 

	make  || die
	make DESTDIR="$pkgdir" install  || die
}
